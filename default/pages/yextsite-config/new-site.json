{
    "$id": "${{site_id}}",
    "$schema": "https://schema.yext.com/config/pages/yextsite-config/v1",
    "siteName": "${{site_name}}",
    "repoConfig": "${{repo_id}}",
    "siteSettings": {
      "autoDeploy": true,
      "autoStage": true,
      "autoPublish": true,
      "pullRequestsEnabled": false,
      "stagingCredentials": {
        "password": "",
        "username": ""
      }
    },
    "branches": [
      {
        "name": "master",
        "isProductionBranch": true,
        "envVars": [
          {
            "YEXT_PUBLIC_LOCATION_ENTITY_ID": "${{YEXT_PUBLIC_LOCATION_ENTITY_ID}}"
          },
          {
            "YEXT_PUBLIC_LOCATION_LOCALE_CODE": "${{YEXT_PUBLIC_LOCATION_LOCALE_CODE}}"
          }
        ]
      }
    ]
  }
  